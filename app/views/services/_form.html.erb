<%= simple_form_for(@service) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :serdescription %>
    <%= f.input :otherSer %>
    <%= f.input :otherSerdescription %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
